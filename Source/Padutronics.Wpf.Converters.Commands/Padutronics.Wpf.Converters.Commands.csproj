<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net7.0-windows</TargetFramework>
        <UseWPF>true</UseWPF>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <WarningLevel>5</WarningLevel>
        <Version>0.1.0</Version>
        <Authors>Pavel Dubsky</Authors>
        <Company>Padutronics</Company>
        <Product>Padutronics Framework</Product>
        <Description>WPF converters for Padutronics.Commands namespace.</Description>
        <Copyright>Copyright Â© Padutronics 2023</Copyright>
        <PackageProjectUrl>https://github.com/Padutronics/Padutronics.Wpf.Converters.Commands</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Padutronics/Padutronics.Wpf.Converters.Commands</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Padutronics.Commands" Version="0.1.0" />
        <PackageReference Include="Padutronics.Commands.Adapters.System.Windows.Input" Version="0.1.0" />
    </ItemGroup>
</Project>